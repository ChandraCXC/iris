#!/bin/sh

bindir=$( cd -P "$( dirname "$0" )" && pwd )
if [ -h "$0" ]; then
    bindir=$( cd -P "$( dirname "`readlink -n "$0"`" )" && pwd )
fi

export IRIS_DIR=$bindir

args="-Xms256m -Xmx1024m -Xdock:name=Iris -Dhttp.agent=${project.name}/${project.version}"

java $args -DIRIS_DIR=$bindir -jar $bindir/${project.name}-${project.version}.jar $@



